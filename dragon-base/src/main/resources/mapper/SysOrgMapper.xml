<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gmtok.dragon.base.mapper.SysOrgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gmtok.dragon.base.entity.SysOrg">
        <id column="id" property="id" />
        <result column="org_code" property="orgCode" />
        <result column="org_name" property="orgName" />
        <result column="org_code_path" property="orgCodePath" />
        <result column="org_level" property="orgLevel" />
        <result column="org_pid" property="orgPid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS id, org_code AS orgCode, org_name AS orgName, org_code_path AS orgCodePath, org_level AS orgLevel, org_pid AS orgPid
    </sql>

</mapper>
